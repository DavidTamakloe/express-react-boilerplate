{
    "name": "express-react-app",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "forever start ./src/server/app.js",
        "build-dev-lib": "webpack --config webpack.lib.dev.js",
        "build-prod-lib": "webpack --config webpack.lib.prod.js",
        "server": "nodemon ./src/server/app.js",
        "client": "npm run build-dev-lib && webpack --watch --config webpack.dev.js",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "build": "npm run build-prod-lib && webpack --config webpack.prod.js && knex migrate:latest"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "bcryptjs": "^2.4.3",
        "connect-mongo": "^3.0.0",
        "cookie-parser": "^1.4.4",
        "csurf": "^1.10.0",
        "debug": "^4.1.1",
        "ejs": "^2.6.2",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "express-validator": "^6.0.0",
        "forever": "^1.0.0",
        "helmet": "^3.18.0",
        "knex": "^0.19.0",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "mongoose": "^5.6.0",
        "morgan": "^1.9.0",
        "numeral": "^2.0.6",
        "objection": "^1.1.10",
        "pg": "7.11.0",
        "randomstring": "^1.1.5",
        "serve-favicon": "^2.5.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.1.9",
        "@storybook/addon-links": "^5.1.9",
        "@storybook/addon-storyshots": "^5.1.9",
        "@storybook/addons": "^5.1.9",
        "@storybook/react": "^5.1.10",
        "autoprefixer": "^9.6.1",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.6",
        "concurrently": "^4.1.1",
        "css-loader": "^3.0.0",
        "eslint": "^5.16.0",
        "eslint-config-defaults": "^9.0.0",
        "eslint-plugin-react": "^7.14.2",
        "file-loader": "^4.0.0",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "nodemon": "^1.19.0",
        "postcss-loader": "^3.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.1",
        "react-test-renderer": "^16.8.6",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0",
        "require-context.macro": "^1.0.4",
        "style-loader": "^0.23.1",
        "url-loader": "^2.0.1",
        "webpack": "^4.35.3",
        "webpack-cli": "^3.3.6",
        "webpack-merge": "^4.2.1"
    }
}
